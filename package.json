{
  "name": "@likou/velocity",
  "version": "0.7.3",
  "description": "A node velocity template engine.",
  "homepage": "https://github.com/likoshow/velocity",
  "keywords": [
    "node",
    "velocity",
    "template",
    "engine"
  ],
  "author": "likou <likoushow@gmail.com>",
  "scripts": {
    "test": "mocha -R spec -t 15000 -r should test/*.test.js",
    "test-cov": "istanbul cover node_modules/.bin/_mocha -- -t 15000 -r should test/*.test.js"
  },
  "dependencies": {
    "colorful": "~2.1.0",
    "commander": "~2.3.0",
    "tracer": "~0.7.3",
    "utilx": "0.0.5"
  },
  "devDependencies": {
    "autod": "*",
    "contributors": "*",
    "istanbul": "*",
    "cov": "*",
    "jshint": "*",
    "mocha": "*",
    "should": "~5.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:likoshow/velocity.git"
  },
  "bin": {
    "velocity": "./bin/velocity"
  },
  "config": {
    "alicov": {
      "threshold": 80
    }
  },
  "preferGlobal": true,
  "license": "MIT"
}
